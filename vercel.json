{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "server-middleware/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 